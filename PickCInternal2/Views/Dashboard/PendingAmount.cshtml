<section class="content">
@using (Html.BeginForm("PaymentsHistory", "Dashboard", FormMethod.Post, new { id = "FrmPaymentsHistory", name = "FrmPaymentsHistory" }))
            {
    <div class="row">
        <div class="col-md-12">
            @{ Html.RenderPartial("DashboardMenuList"); }
                                </div>
                            </div>
        <div class="row" style="margin:15px 0 0px -30px;">
                <div class="col-md-12">
                    <div class="col-md-6">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <h2>User Pending Cancellation Charges</h2>
                                            <div class="table-responsive">
                                                <table id="Table_userpending" class="table table-hover table-bordered">
                                                    <thead>
                                                        <tr>
                                                            <th>Booking No.</th>
                                                            <th>Booking Date</th>
                                                            <th>Vehicle Type</th>
                                                            <th>Vehicle No.</th>
                                                            <th>Location From</th>
                                                            <th>Location To</th>
                                                            <th>Cancellation Remarks</th>
                                                            <th>Cancellation Amount</th>
                                                        </tr>
                                                    </thead>
                                                    @*@<tbody>
                            @foreach(var item in )
                    {
                        <tr>
                            <td>@Html.DisplayFor(i=>item.BookingNo)</td>
                            <td>@Html.DisplayFor(i => item.BookingDate)</td>
                            <td>@Html.DisplayFor(i => item.VehicleType)</td>
                            <td>@Html.DisplayFor(i => item.VehicleNo)</td>
                            <td>@Html.DisplayFor(i => item.LocationFrom)</td>
                            <td>@Html.DisplayFor(i => item.LocationTo)</td>
